<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="#b9e8c0">

    <item>
        <selector>

        <item android:state_pressed="false">
            <shape>
                <!-- 填充的颜色 -->
                <solid android:color="#fff"/>
                <!-- 设置按钮的四个角为弧形 -->
                <!-- android:radius 弧形的所有半径 -->
                <corners android:radius="5dp"/>
                <!-- 边框的宽度及颜色 -->
                <stroke
                    android:width="0.1dp"
                    android:color="#fff"/>
            </shape>
        </item>
        <!--true 为按下的时候-->

        </selector>
    </item>
</ripple>